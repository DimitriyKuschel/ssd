# SSD Docker Configuration
# Copy this file to .env and adjust values as needed:
#   cp .env.sample .env

# Host port mapping
SSD_PORT=8090

# Data and logs directories on the host machine
SSD_DATA_DIR=./data
SSD_LOGS_DIR=./logs

# Application settings
SSD_LOG_LEVEL=info
SSD_AGGREGATION_INTERVAL=60
SSD_SAVE_INTERVAL=120
