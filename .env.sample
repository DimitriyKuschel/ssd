# SSD Docker Configuration
# Copy this file to .env and adjust values as needed:
#   cp .env.sample .env

# Host port mapping
SSD_PORT=8090

# Data and logs directories on the host machine
SSD_DATA_DIR=./data
SSD_LOGS_DIR=./logs

# Application settings
SSD_LOG_LEVEL=info
SSD_AGGREGATION_INTERVAL=60s
SSD_SAVE_INTERVAL=120s

# Cache settings
SSD_CACHE_ENABLED=true
SSD_CACHE_SIZE=32

# Metrics settings
SSD_METRICS_ENABLED=true
